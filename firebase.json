{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ]
  },
  "hosting": {
    "public": "website",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
	  {
		"source": "/addImmuneHero",
        "function": "addImmuneHero"
	  },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
